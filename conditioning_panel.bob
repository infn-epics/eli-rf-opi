  <widget type="action_button" version="3.0.0">
    <name>expert_panel_button</name>
    <actions>
      <action type="execute">
        <description>Execute Script</description>
        <script file="EmbeddedPy">
          <text><![CDATA[from org.csstudio.display.builder.runtime.script import PVUtil, ScriptUtil
from org.csstudio.opibuilder.scriptUtil import ColorFontUtil

expert_panel = ScriptUtil.findWidgetByName(widget, 'Expert_Panel')
status=expert_panel.getPropertyValue('visible')
print(status)
if status==1:
	expert_panel.setPropertyValue('visible',False)
	widget.setPropertyValue('text',"Open Expert Panel")
	widget.setPropertyValue('background_color',ColorFontUtil.GRAY)
	widget.setPropertyValue('show_confirm_dialog',True)
else:
	expert_panel.setPropertyValue('visible',True)
	widget.setPropertyValue('text',"Close Expert Panel")
	widget.setPropertyValue('background_color',ColorFontUtil.RED)
	widget.setPropertyValue('show_confirm_dialog',False)
]]></text>
        </script>
      </action>
    </actions>
