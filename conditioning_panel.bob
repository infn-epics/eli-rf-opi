      <scripts>
        <script file="EmbeddedPy" check_connections="false">
          <text><![CDATA[from org.csstudio.display.builder.runtime.script import PVUtil
from org.csstudio.display.builder.model.properties import WidgetColor

data = []
labels = PVUtil.getStringArray(pvs[0])
tsh_values = PVUtil.getDoubleArray(pvs[1])

for i, pv in enumerate(pvs[2:2+len(labels)]):  
    if i < len(labels):  
        row = [labels[i], str(PVUtil.getDouble(pv)), str(tsh_values[i])]
        data.append(row)

widget.setValue(data)

for i, pv in enumerate(pvs[2:2+len(labels)]): 
    vacuum_value = PVUtil.getDouble(pv)
    if vacuum_value >= tsh_values[i]:
        widget.setCellColor(i, 0, WidgetColor(255, 0, 0))  # Rosso se uguale alla soglia
        widget.setCellColor(i, 1, WidgetColor(255, 0, 0))
	widget.setCellColor(i, 2, WidgetColor(255, 0, 0))
    elif vacuum_value > tsh_values[i] * 0.9:
        widget.setCellColor(i, 0, WidgetColor(255, 255, 50))  # Giallo se maggiore del 90%
        widget.setCellColor(i, 1, WidgetColor(255, 255, 50))
	widget.setCellColor(i, 2, WidgetColor(255, 255, 50))]]></text>
